void sortDishPrice() { 
    for (int i = 0; i < number - 1; i++) { 
        for (int j = i + 1; j < number; j++) { 
            if (menu[i].price > menu[j].price) { 
                dish tmp = menu[i]; 
                menu[i] = menu[j]; 
                menu[j] = tmp; 
            } 
        } 
    } 
    printf("Sap xep xong.\n"); 
} 
